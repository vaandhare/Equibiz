<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.birdvision.equibiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/NoActionBarTheme">
        <activity android:name=".userInfo.VerificationActivity"></activity>
        <activity android:name=".userInfo.BusinessDetailsActivity" />
        <activity android:name=".product.ProductActivity" />
        <activity android:name=".product.Category1Activity" />
        <activity android:name=".category.AllCategoryActivity" />
        <activity
            android:name=".userInfo.UserProfile"
            android:parentActivityName=".MainActivity"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name=".userInfo.RegisterActivity"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name=".userInfo.LoginActivity"
            android:theme="@style/NoActionBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/NoActionBarTheme" />
    </application>

</manifest>